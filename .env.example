# Variáveis de ambiente para o projeto
PORT=5173
NODE_ENV=development

# API Base URL para o backend Django
VITE_API_BASE_URL=http://127.0.0.1:8000

# Credenciais de autenticação JWT
VITE_API_EMAIL=seu_email@example.com
VITE_API_PASSWORD=sua_senha_secreta

# Controle de comportamento da API
# Definir como 'true' para forçar o uso dos dados mock independente da disponibilidade da API
VITE_FORCE_MOCK_API=false
# Definir como 'true' para mostrar logs detalhados das chamadas de API
VITE_DEBUG_API=true